div.jp-single-player,
div.jp-playlist-player {
	font-size:1em; /* No parent CSS that can effect the size in these demos */
	font-family:Verdana, Arial, sans-serif;
	line-height:1.6;
	color: #666;
}
div.jp-interface {position: relative;background-color:#eaeaea;	width:390px;height:20px; border-bottom:1px solid #888;}
div.jp-single-player div.jp-interface {padding: 3px 1px;}

div.jp-interface ul.jp-controls {
	list-style-type:none;
	padding:0;
	margin: 0;
}
div.jp-interface ul.jp-controls li {
	position: absolute;
}
div.jp-interface ul.jp-controls a {
	display: block;
	position: absolute;
	overflow:hidden;
	text-indent:-9999px;
}
a.jp-play,a.jp-pause,a.jp-stop,a.jp-next {top:2px;width:16px;height:16px;}
div.jp-single-player a.jp-play,
div.jp-single-player a.jp-pause {
	left:5px;
}
a.jp-play {background: url("play.png") no-repeat;}
a.jp-play:hover {background: url("play-h.png") no-repeat;}
a.jp-pause {background: url("pause.png") no-repeat;}
a.jp-pause:hover {background: url("pause-h.png") no-repeat;}
a.jp-stop {background: url("stop.png") no-repeat; left: 25px;}
a.jp-stop:hover {background: url("stop-h.png") no-repeat;}
a.jp-next {background: url('next.png') no-repeat; left:45px; width: 19px;}
a.jp-next:hover {background: url('next-h.png') no-repeat;}

div.jp-progress {position: absolute;overflow:hidden; top:8px; background-color: #ddd; width:130px; height:10px; left:105px;}
div.jp-load-bar {background: url("jplayer.blue.monday.jpg") 0 -202px repeat-x; width:0px; height:15px; cursor: pointer;}

div.jp-play-bar {background: url("jplayer.blue.monday.jpg") 0 -218px repeat-x ; width:0px; height:15px;}
a.jp-volume-min {top:2px;	background: url("jplayer.blue.monday.jpg") 0 -170px no-repeat;	width:18px;	height:15px;}
div.jp-single-player a.jp-volume-min {left:275px;}
a.jp-volume-min:hover {background: url("jplayer.blue.monday.jpg") -19px -170px no-repeat;}
a.jp-volume-max {top:2px;	background: url("jplayer.blue.monday.jpg") 0 -186px no-repeat;	width:18px;	height:15px;}
div.jp-single-player a.jp-volume-max {left:350px;}
a.jp-volume-max:hover {background: url("jplayer.blue.monday.jpg") -19px -186px no-repeat;}

div.jp-volume-bar {
	position: absolute;
	overflow:hidden;
	top:10px;
	background: url("jplayer.blue.monday.jpg") 0 -250px repeat-x;
	width:46px;
	height:5px;
	cursor: pointer;
}
div.jp-single-player div.jp-volume-bar {
	left:294px;
}
div.jp-volume-bar-value {
	background: url("jplayer.blue.monday.jpg") 0 -256px repeat-x;
	width:0px;
	height:5px;
}
div.jp-play-time, div.jp-total-time {position: absolute;top:4px;font-size:10px;font-style:oblique;}
div.jp-total-time {text-align: right;}
div.jp-single-player div.jp-play-time {left: 70px;}
div.jp-single-player div.jp-total-time {left: 240px;}
div.jp-playlist {width:418px;}
div.jp-playlist ul{
	list-style-type:none;
	margin:0;
	padding:0 20px;
	background-color:#ccc;
	border:1px solid #009be3;
	border-top:none;
	width:378px;
	font-size:.72em;
}
div.jp-single-player div.jp-playlist li {
	padding:5px 0 5px 20px;
	font-weight:bold;
}
div.jp-playlist-player div.jp-playlist li {
	padding:5px 0 4px 20px;
	border-bottom:1px solid #eee;
}
div.jp-playlist-player div.jp-playlist li.jplayer_playlist_item_last {
	padding:5px 0 5px 20px;
	border-bottom:none;
}
div.jp-playlist-player div.jp-playlist li.jplayer_playlist_current {
	list-style-type:square;
	list-style-position:inside;
	padding-left:8px;
}
div.jp-playlist-player div.jp-playlist a {
	color: #666;
	text-decoration: none;
}
div.jp-playlist-player div.jp-playlist a:hover {
	color:#0d88c1;
}
div.jp-playlist-player div.jp-playlist a.jplayer_playlist_current {
	color:#0d88c1;
}
